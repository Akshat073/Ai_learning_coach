{
    "version": 2,
    "name": "Kid tutor",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/"
        }
      ],
      "buildCommand": "npm run build",
      "outputDirectory": "dist"
    
}