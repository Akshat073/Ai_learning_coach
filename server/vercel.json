{
    "version": 2,
    "name": "Kid tutor",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }] ,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}